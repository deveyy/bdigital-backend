{
    "root": true,
    "extends": [
        "react-app",
        "react-app/jest",
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
      ],
    "plugins": [
        "react","prettier"
    ],
    "rules": {
        "semi":"warn",
        "strict": "off",
        "react/no-set-state": "off",
        "no-unused-vars": ["off", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}